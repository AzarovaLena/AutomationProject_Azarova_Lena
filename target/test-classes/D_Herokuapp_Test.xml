<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Welcome to the-internet" parallel="tests">
    <parameter name="env" value="herokuapp"></parameter>
    
    <test name="Context Menu">
        <classes>
            <class name="pageObject_Test.herokuapp_Test.HomePage_Test"></class>
            <class name="pageObject_Test.herokuapp_Test.ContextMenu_Test"></class>
        </classes>
    </test>
    <test name="Dynamic Controls">
        <classes>
            <class name="pageObject_Test.herokuapp_Test.HomePage_Test"></class>
            <class name="pageObject_Test.herokuapp_Test.DynamicControls_Test"></class>
        </classes>
    </test>
    <test name="File Uploader">
        <classes>
            <class name="pageObject_Test.herokuapp_Test.HomePage_Test"></class>
            <class name="pageObject_Test.herokuapp_Test.FileUploader_Test"></class>
        </classes>
    </test>

    <test name="Frames">
        <classes>
            <class name="pageObject_Test.herokuapp_Test.HomePage_Test"></class>
            <class name="pageObject_Test.herokuapp_Test.Frames_Test"></class>
        </classes>
    </test>
</suite>