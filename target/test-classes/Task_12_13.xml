<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Welcome to the-internet" parallel="tests">
    <parameter name="env" value="herokuapp"></parameter>
    
    <test name="Context Menu">
        <classes>
            <class name="Lecture_12_13.Task_12_13_homePage"></class>
            <class name="Lecture_12_13.Task_12_13_contextMenu"></class>
        </classes>
    </test>
    <test name="Dynamic Controls">
        <classes>
            <class name="Lecture_12_13.Task_12_13_homePage"></class>
            <class name="Lecture_12_13.Task_12_13_dynamicControls"></class>
        </classes>
    </test>
    <test name="File Uploader">
        <classes>
            <class name="Lecture_12_13.Task_12_13_homePage"></class>
            <class name="Lecture_12_13.Task_12_13_fileUploader"></class>
        </classes>
    </test>

    <test name="Frames">
        <classes>
            <class name="Lecture_12_13.Task_12_13_homePage"></class>
            <class name="Lecture_12_13.Task_12_13_frames"></class>
        </classes>
    </test>
</suite>